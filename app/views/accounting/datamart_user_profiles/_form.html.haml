    = f.text_field :effective_date, class: 'datepicker'

.control-group
  = f.label :preferred_first_name, class: 'control-label'
  .controls
    = f.text_field :preferred_first_name

.control-group
  = f.label :ultipro_emp_id, class: 'control-label'
  .controls
    = f.text_field :ultipro_emp_id
