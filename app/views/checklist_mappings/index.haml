.page-header
  %h1 Checklist Mappings
  
  %h2 Closing Checklist

  %table{:class => 'table table-striped table-bordered'}
    %thead
      %tr
        %th Question
        %th Name
    %tbody
      - @closing_questions.each do |question|
        %tr
          %td= question.options[:prompt]
          %td= question.name
